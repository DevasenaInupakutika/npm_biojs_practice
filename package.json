{
  "name": "npm_biojs_practice",
  "version": "0.0.0",
  "description": "Browserify npm module BioJS component example",
  "main": "index.js",
  "scripts": {
    "build-debug": "browserify index.js -d --s pizza > dist/pizza.js",
    "build-min": "browserify index.js --s pizza | uglifyjs -c > dist/pizza.min.js",
    "build": "npm run build-debug && npm run build-min",
    "watch": "watchify index.js -d --s pizza -o dist/pizza.js -v"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/DevasenaInupakutika/npm_biojs_practice.git"
  },
  "keywords": [
    "Browserify"
  ],
  "author": "Devasena",
  "license": "CC",
  "bugs": {
    "url": "https://github.com/DevasenaInupakutika/npm_biojs_practice/issues"
  },
  "homepage": "https://github.com/DevasenaInupakutika/npm_biojs_practice",
  "devDependencies": {
    "uglify-js": "~2.4.15",
    "watchify": "~1.0.2",
    "browserify": "~5.11.0"
  }

}
